{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/root/cesium-terrain-builder/build/tools/ctb-tile",
            "args": [
                "--output-dir",
                "/root/cesium-terrain-builder/data/tiles",
                "--extensions",
                "/root/cesium-terrain-builder/data/extensions/Isopach_UB.tif /root/cesium-terrain-builder/data/extensions/OIP_UB.tif",
                "/root/cesium-terrain-builder/data/0_UB_e.tif",
                "-f",
                "Mesh",
                "-C",
                "-N"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}